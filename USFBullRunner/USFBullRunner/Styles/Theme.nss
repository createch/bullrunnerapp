@primaryFontName: GeezaPro;
@secondaryFontName: GeezaPro;
@primaryFontColor: #333333;
@primaryBackgroundColor: #08B77E;

NavigationBar {
    background-color: @primaryBackgroundColor;
    font-name: @primaryFontName;
    font-size: 20;
    font-color: #ffffff;
    corner-radius: 50;
    text-align:left;
}

TableCell {
    font-name: @secondaryFontName;
    background-color: #E14D42;
}